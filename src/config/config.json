{
    "database": {
        "connection_string": "postgresql://dance_user:dance_password@postgres:5432/dance_studio",
        "max_connections": 10,
        "connection_timeout_seconds": 30
    },
    "business_logic": {
        "max_booking_days_ahead": 30,
        "lesson_cancellation_hours": 24,
        "max_participants_per_lesson": 50,
        "default_lesson_duration_minutes": 60
    },
    "logging": {
        "level": "INFO",
        "file_path": "logs/dance_studio.log",
        "max_file_size_mb": 10,
        "backup_count": 5
    },
    "application": {
        "name": "Dance Studio Management System",
        "version": "1.0.0",
        "tech_ui_enabled": true
    }
}